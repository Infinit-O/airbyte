{
  "documentationUrl": "https://docsurl.com",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Desktop Central Spec",
    "type": "object",
    "required": ["username", "password", "otp_key", "base_url"],
    "additionalProperties": false,
    "properties": {
      "username": {
        "order": 0,
        "type": "string"
      },
      "password": {
        "order": 1,
        "type": "string",
        "airbyte_secret": true
      },
      "otp_key": {
        "order": 2,
        "type": "string",
        "airbyte_secret": true
      },
      "base_url": {
        "order": 3,
        "type": "string"
      }
    }
  }
}
