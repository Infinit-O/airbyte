                    "type": "string",
                    "example": "Fri Nov 03, 2017"
                }
            }
        },
        "purchase_cost": {
            "type": "string",
            "example": "1114.86"
        },
        "user_can_checkout": {
            "type": "boolean",
            "example": false,
            "default": true
        },
        "custom_fields": {
            "type": "array"
        },
        "available_actions": {
            "type": "object",
            "properties": {
                "checkout": {
                    "type": "boolean",
                    "example": true,
                    "default": true
                },
                "checkin": {
                    "type": "boolean",
                    "example": true,
                    "default": true
                },
                "clone": {
                    "type": "boolean",
                    "example": true,
                    "default": true
                },
                "update": {
                    "type": "boolean",
                    "example": true,
                    "default": true
                },
                "delete": {
                    "type": "boolean",
                    "example": true,
                    "default": true
                }
            }
