                "formatted": {
                    "type": "string",
                    "example": "Wed Oct 18, 2017 9:00PM"
                }
            }
        },
        "updated_at": {
            "type": "object",
            "properties": {
                "datetime": {
                    "type": "string",
                    "example": "2017-10-19 07:57:37"
