                    "type": "string",
                    "example": "admin"
                },
                "name": {
                    "type": "string",
                    "example": "Admin User"
                },
                "first_name": {
                    "type": "string",
                    "example": "Admin"
                },
                "last_name": {
                    "type": "string",
                    "example": "User"
                },
                "employee_number": {
                    "type": "string",
                    "example": "19135"
                },
                "type": {
                    "type": "string",
                    "example": "user"
                }
            }
