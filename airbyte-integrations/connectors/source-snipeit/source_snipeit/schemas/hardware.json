                "name": {
                    "type": "string",
                    "example": "Deployed"
                },
                "status_meta": {
                    "type": "string",
                    "example": "deployed"
                }
            }
        },
        "category": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer",
                    "example": 1,
                    "default": 0
                },
                "name": {
                    "type": "string",
                    "example": "Laptops"
                }
            }
        },
        "manufacturer": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer",
                    "example": 1,
                    "default": 0
                },
                "name": {
                    "type": "string",
                    "example": "Apple"
                }
            }
        },
        "supplier": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer",
                    "example": 1,
                    "default": 0
