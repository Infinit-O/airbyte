{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "name": "users",
    "properties": {
        "id": {"type": "integer"},
        "avatar": {"type": "string"},
        "name": {"type": "string"},
        "first_name": {"type": "string"},
        "last_name": {"type": "string"},
        "username": {"type": "string"},
        "locale": {"type": ["string", "null"]},
        "employee_num": {"type": ["string", "null"]},
        "manager": {"type": ["string", "null"]},
        "jobtitle": {"type": ["string", "null"]},
        "phone": {"type": ["string", "null"]},
        "website": {"type": ["string", "null"]},
        "address": {"type": ["string", "null"]},
        "city": {"type": ["string", "null"]},
        "state": {"type": ["string", "null"]},
        "country": {"type": ["string", "null"]},
        "zip": {"type": ["string", "null"]},
        "email": {"type": ["string", "null"]},
        "department": {"type": ["object", "null"]},
        "location": {
            "type": ["object", "null"],
            "properties": {
                "id": {
                    "type": "integer",
                    "example": 3,
                    "default": 0
                },
                "name": {
                    "type": "string",
                    "example": "Nikitaborough"
                }

            }
        },
        "notes": {"type": "string"},
        "permissions": {"type": ["object", "null"]},
        "activated": {"type": "boolean"},
        "ldap_import": {"type": "boolean"},
        "two_factor_activated": {"type": "boolean"},
        "two_factor_enrolled": {"type": "boolean"},
        "assets_count": {"type": "integer"},
        "licenses_count": {"type": "integer"},
        "accessories_count": {"type": "integer"},
        "consumables_count": {"type": "integer"},
        "company": {"type": ["object", "null"]},
        "created_at": {
            "type": "object",
            "properties": {
                "datetime": {
                    "type": "string",
                    "example": "2018-10-09 19:09:21"
                },
                "formatted": {
                    "type": "string",
                    "example": "2018-10-09 07:09 PM"
                }
            }
        },
        "updated_at": {
            "type": "object",
            "properties": {
                "datetime": {
                    "type": "string",
                    "example": "2018-10-09 19:09:21"
                },
                "formatted": {
                    "type": "string",
                    "example": "2018-10-09 07:09 PM"
                }
            }
        },
        "last_login": {
            "type": ["object", "null"],
            "properties": {
                "datetime": {
                    "type": "string",
                    "example": "2018-10-09 19:09:21"
                },
                "formatted": {
                    "type": "string",
                    "example": "2018-10-09 07:09 PM"
                }
            }
        },
        "deleted_at": {
            "type": ["object", "null"],
            "properties": {
                "datetime": {
                    "type": "string",
                    "example": "2018-10-09 19:09:21"
                },
                "formatted": {
                    "type": "string",
                    "example": "2018-10-09 07:09 PM"
                }
            }
        },
        "available_actions": {
            "type": "object",
            "properties": {
                "checkout": {
                    "type": "boolean",
                    "example": true,
                    "default": true
                },
                "checkin": {
                    "type": "boolean",
                    "example": true,
                    "default": true
                },
                "update": {
                    "type": "boolean",
                    "example": true,
                    "default": true
                },
                "delete": {
                    "type": "boolean",
                    "example": true,
                    "default": true
                }
            }
        },
        "groups": {"total": "object"}
    }
}
