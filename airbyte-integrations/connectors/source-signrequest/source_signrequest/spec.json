{
  "documentationUrl": "https://docsurl.com",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Signrequest Spec",
    "type": "object",
    "required": ["access_token", "url_base"],
    "additionalProperties": false,
    "properties": {
      "access_token": {
        "type": "string",
        "description": "API access token provided by the admin."
      
      },
      "url_base": {
        "type": "string",
        "description": "The base url for the team you're trying to collect data from."
      
      }
    }
  }
}
